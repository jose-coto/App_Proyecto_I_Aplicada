<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context="cr.ac.ucr.app_proyecto_i_aplicada.CategoryListFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="cr.ac.ucr.app_proyecto_i_aplicada.CategoryListActivity">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorPrimaryDark"
            android:layout_below="@+id/toolbarCategoryList"
            android:orientation="vertical">

            <ListView
                android:id="@+id/lvCategoryList"
                style="@style/Widget.AppCompat.ListView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/colorPrimaryDark"
                android:footerDividersEnabled="false"
                android:headerDividersEnabled="false" />

        </LinearLayout>


    </RelativeLayout>

</FrameLayout>
